<configuration>
  <configSections>
    <section name="nlog" type="NLog.Config.ConfigSectionHandler, NLog" />
    <section name="entityFramework" type="System.Data.Entity.Internal.ConfigFile.EntityFrameworkSection, EntityFramework, Version=6.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" requirePermission="false" />
  </configSections>
  <nlog xmlns="http://www.nlog-project.org/schemas/NLog.xsd"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      autoReload="true">
    <targets>
      <default-target-parameters xsi:type="File" keepFileOpen="false" />
      <target name="asyncFile" xsi:type="AsyncWrapper" queueLimit="1000" overflowAction="Discard">
        <target xsi:type="File"
                name="logfile"
                fileName="${basedir}/Logs/${level}-${shortdate}.log"
                cleanupFileName="false"
                header="${appdomain} ${assembly-version}"
                layout="${newline}-------------------------------------${newline}Fecha: ${date:format=yyyy-MM-dd HH\:mm\:ss.fff} ${newline}${newline}${uppercase:${level}} ${longdate}${newline} Usuario: ${windows-identity}${newline} ${machinename}${newline}${callsite} ${newline} Message: ${message}${newline}${onexception:${exception:format=tostring,Data}${newline}${stacktrace}}" />
      </target>
    </targets>
    <rules>
      <logger name="*" minlevel="Warn" writeTo="asyncFile" />
    </rules>
  </nlog>
  <runtime>
    <gcServer enabled="true" />
  </runtime>
  <appSettings>
    <add key="EdadMayorEdad" value="18" />
  </appSettings>
  <connectionStrings>
    <add name="AerolineaTnsEntities" connectionString="metadata=res://*/Model.AerolineaTns.csdl|res://*/Model.AerolineaTns.ssdl|res://*/Model.AerolineaTns.msl;provider=System.Data.SqlClient;provider connection string=&quot;data source=.\sqlexpress;initial catalog=AerolineaTNS;persist security info=True;user id=tns;password=tN$u$er;MultipleActiveResultSets=True;App=EntityFramework&quot;" providerName="System.Data.EntityClient" />
  </connectionStrings>
  <entityFramework>
    <defaultConnectionFactory type="System.Data.Entity.Infrastructure.SqlConnectionFactory, EntityFramework" />
    <providers>
      <provider invariantName="System.Data.SqlClient" type="System.Data.Entity.SqlServer.SqlProviderServices, EntityFramework.SqlServer" />
    </providers>
  </entityFramework>
</configuration>